{
  "name": "fifty-two-hours",
  "version": "0.0.0",
  "description": "Application to Manage Working Hours",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "webpack --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config ./webpack.config.dev.js --hot",
    "preelectron-pack": "npm build",
    "electron-pack": "build -mw",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:8080 && electron .\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalgos/52hours.git"
  },
  "keywords": [
    "electron"
  ],
