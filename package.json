    "electron-is-dev": "^1.1.0",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "ramda": "^0.26.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.15",
