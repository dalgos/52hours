    "name": "kei shin",
    "email": "function.bind@gmail.com",
    "url": "https://createweb.app"
  },
  "build": {
    "appId": "app.createweb.fifty-two-hours",
    "productName": "52Hours",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dalgos/52hours/issues"
  },
